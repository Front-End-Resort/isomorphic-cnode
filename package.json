{
    "name": "isomorphic-cnode",
    "version": "3.0.0",
    "description": "inspired by [Vue-cnodejs](https://github.com/shinygang/Vue-cnodejs), rewrited with `react-imvc`",
    "private": true,
    "scripts": {
        "start": "react-imvc start --config ./imvc.config.ts",
        "debug": "react-imvc start?inspect-brk --config ./imvc.config.ts",
        "build": "react-imvc build --config ./imvc.config.ts",
        "delete": "rimraf ./node_modules/create-history&rimraf node_modules/create-app&rimraf node_modules/relite&rimraf node_modules/react-imvc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Front-End-Resort/isomorphic-cnode.git"
    },
