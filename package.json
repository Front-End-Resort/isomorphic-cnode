        "url": "https://github.com/Front-End-Resort/isomorphic-cnode/issues"
    },
    "homepage": "https://github.com/Front-End-Resort/isomorphic-cnode#readme",
    "dependencies": {
        "@types/classnames": "^2.2.9",
        "@types/express": "^4.17.3",
        "@types/node": "^12.12.30",
        "@types/react": "^16.9.25",
        "@types/react-dom": "^16.9.5",
        "debug": "^4.1.1",
        "markdown": "^0.5.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-imvc": "^3.0.0",
        "split-on-first": "^1.1.0",
